{
  "name": "tsc-starter",
  "version": "1.0.0",
  "description": "A Nodejs & Typescript API template",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "npm run && concurrently -r \"npm run tsc-w\" \"npm run node\" ",
    "node": "nodemon -q ./dist/server.js",
    "tsc": "tsc",
    "tsc-w": "tsc -w",
    "test": " tsc && mocha ./dist/test/*.js"
  },
  "keywords": [
    "Nodejs",
    "Typescript",
    "Starter",
    "API"
  ],
  "author": "Fernando Quintana",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.17",
    "express-validator": "^6.4.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.6",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "mocha": "^7.1.1",
    "mongodb-memory-server-core": "^6.5.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3"
  }
}
